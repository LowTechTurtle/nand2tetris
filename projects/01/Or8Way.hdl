/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
        Or (a = in[0], b = in[1], out = in1);
        Or (a = in1, b = in[2], out = in2);
        Or (a = in2, b = in[3], out = in3);
        Or (a = in3, b = in[4], out = in4);
        Or (a = in4, b = in[5], out = in5);
        Or (a = in5, b = in[6], out = in6);
        Or (a = in6, b = in[7], out = out);
}
